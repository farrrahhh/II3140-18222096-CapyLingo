{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/signup",
        "dest": "/index.js"
      },
      {
        "src": "/api/login",
        "dest": "/index.js"
      },
      {
        "src": "/api/submit-quiz",
        "dest": "/index.js"
      },
      {
        "src": "/api/quizzes",
        "dest": "/index.js"
      },
      {
        "src": "/api/verify-token",
        "dest": "/index.js"
      }
    ]
  }
  